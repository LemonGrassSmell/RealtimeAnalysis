# Log
log.dir=/Users/Jelly/Developer/RealTimeAnalysis/
logs=system.log,user.log

# LoaderClient
## Zookeeper
acks=1
retries=0
batch.size=1024
buffer.memory=33554432
bootstrap.servers=127.0.0.1:9092
linger.ms=1
key.serializer=org.apache.kafka.common.serialization.LongSerializer
value.serializer=cn.edu.ruc.realtime.utils.MessageSer
partitioner.class=cn.edu.ruc.realtime.partition.LoaderClientPartitionKafka

## LoaderClient ThreadPool
thread.pool.size=8
thread.queue.size=1000

# Loader
## LoaderThread
bootstrap.servers=127.0.0.1:9092
consumer.group.id=test
consumer.auto.commit=false
consumer.auto.commit.interval.ms=1000
consumer.session.timeout=30000
consumer.key.deserializer=org.apache.kafka.common.serialization.LongDeserializer
consumer.value.deserializer=cn.edu.ruc.realtime.utils.MessageDer
writer.batch.size=100

## Blocking Queue
blocking.queue.size=12288

## WriterThread
writer.block.size=10
writer.full.factor=0.98
writer.file.path=/Users/Jelly/Developer/RealTimeAnalysis/
writer.thread.num=1